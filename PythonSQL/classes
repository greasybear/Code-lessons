class Movie:
    def __init__(self, name, genre):
        self.name = name
        self.genre = genre
    def __repr__(self):
        return "<Movie {}>".format(self.name)
     
class User:
    def __init__(self, name):
        self.name=name
        self.movies = []
    def __repr__(self):
        return "<User {}>".format(self.name)

user = User('Andrew')

myMovie= Movie('The Matrix', 'Sci-Fi')

user.movies.append(myMovie)


print(user)
print(user.movies)
